                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Home_tempActivity" />
        <activity android:name=".SignUpActivity" />
        <activity android:name=".ChangePasswordActivity" />
        <activity android:name=".DeleteAccountActivity" />
        <activity android:name=".FindPasswordActivity" />
        <activity
            android:name=".HomeActivity"
            android:label=""
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".AirHistoryActivity" />
        <activity android:name=".HeartRateHistoryActivity" />
